{
  "name": "@nurl-inc/website",
  "type": "module",
  "scripts": {
    "clean": "rm -rf src/data/generated",
    "generate": "tsx ./scripts/generate.ts",
    "dev": "pnpm run clean && pnpm run generate && vinxi dev",
    "build": "pnpm run clean && pnpm run generate && vinxi build",
    "start": "pnpm run clean && pnpm run generate && vinxi start",
    "prepare": "husky && panda codegen",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint ."
  },
  "dependencies": {
    "@ark-ui/solid": "^5.26.0",
    "@captchafox/solid": "^1.5.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.2.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^2.2.7",
    "motion": "^12.23.24",
    "remove": "^0.1.5",
    "resend": "^6.2.0",
    "solid-js": "^1.9.9",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@ark-ui/anatomy": "^3.5.0",
    "@eslint/js": "^9.38.0",
    "@pandacss/dev": "^1.4.1",
    "@pandacss/types": "^1.4.1",
    "@react-email/render": "^1.4.0",
    "@types/marked": "^6.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.5",
    "prettier": "3.6.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-directive": "^4.0.0",
    "remark-directive-rehype": "^0.4.2",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.1.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "unified": "^11.0.5",
    "vinxi": "^0.5.8",
    "vite": "^6.2.7",
    "vite-plugin-sitemap": "^0.8.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "packageManager": "pnpm@10.18.0",
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.0",
      "estree-util-value-to-estree": "3.3.3",
      "brace-expansion": "1.1.12"
    }
  }
}
